#------------------------------------------------------------------------------
# Util functions
#------------------------------------------------------------------------------

timestamp() {
    date "+%Y-%m-%d %H:%M:%S"
}

print_with_color() {
    echo -e "$2[`timestamp`] $1$COLOR_RESET"
}

export_env() {
    export "$1=$2";
    print_with_color "Environment variable \"$1\" setted to \"$2\"" $GREEN;
}
